{
  "version": 2,
  "routes": [
    {
      "src": "/.*",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-ip-country",
          "value": "TZ"
        }
      ],
      "dest": "https://kutombana.vercel.app/"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
